{
  "kind": "collectionType",
  "collectionName": "instances",
  "info": {
    "singularName": "instance",
    "pluralName": "instances",
    "displayName": "Instance",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "account": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::account.account",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "acronym": {
      "type": "string"
    },
    "subdomain": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 200,
      "unique": true
    },
    "instanceUUID": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "started",
        "stopped"
      ]
    },
    "webhooks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.webhook",
      "mappedBy": "instance"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "instance"
    },
    "default_locale": {
      "type": "string",
      "minLength": 2,
      "default": "en"
    },
    "available_locales": {
      "type": "string",
      "default": "en,fr,pt-BR",
      "minLength": 2
    },
    "timezone": {
      "type": "string",
      "default": "UTC",
      "minLength": 3
    },
    "currency": {
      "type": "string",
      "default": "â‚¬",
      "minLength": 1
    },
    "creator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "envName": {
      "type": "string",
      "maxLength": 25,
      "unique": true
    },
    "customDomain": {
      "type": "string"
    }
  }
}
